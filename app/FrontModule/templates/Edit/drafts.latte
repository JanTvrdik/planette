{*
	Page : Edit/Drafts
*}

{block title}{_ ui.drafts}{/block}
{block #content}
<div class="container">

	<div class="row">
		<div class="col-sm-8 col-sm-offset-2">
			<h2>{_ ui.drafts}</h2>

			<ul>
				{foreach $draftsList as $draft}
